[Error]
enum ArithmeticError {
  "IntegerOverflow",
};

[Error]
enum FalconError {
  "FalconKeygenFailed",
};

dictionary FalconKeyPair {
    bytes public_key;
    bytes private_key;
};


namespace arithmetic {
  [Throws=ArithmeticError]
  u64 add(u64 a, u64 b);

  [Throws=ArithmeticError]
  u64 sub(u64 a, u64 b);

  u64 div(u64 dividend, u64 divisor);

  boolean equal(u64 a, u64 b);

  [Async]
  string say_after(u64 ms, string who);

  [Async]
  string http_get(string url);

  bytes genkey();

  [Throws=FalconError]
  FalconKeyPair falcon_genkey(bytes seed);
};
